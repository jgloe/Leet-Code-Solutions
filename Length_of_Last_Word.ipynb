   "source": [
    "Leet code problem\\\n",
    "Given a string separated by spaces, return the length of the last word"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "def lengthOfLastWord(s):\n",
    "    l = s.split()\n",
    "    last_word = l[-1]\n",
    "    return len(last_word)"
   ]
  },
